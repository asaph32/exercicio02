import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    float fatorial=0;
    float divisor=0;
    Scanner teclado = new Scanner (System.in);
    System.out.println("Informe um numero inteiro e positivo : ");
    float m = teclado.nextFloat();
    if ( m <= 10) {
      fatorial=m*(m-1)*(m-2)*(m-3)*(m-4);
    } 
    System.out.println("O numero fatorial dele Ã© de : "+fatorial);
    
  }
}
