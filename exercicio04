import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    float S;
    Scanner teclado = new Scanner (System.in);
    System.out.println("Informe um numero inteiro positivo:");
    float n = teclado.nextFloat();
    n=n*(n-1)*(n-2)*(n-3)*(n-4);
    S = 1 + 1/1 + 1/2 + 1/6 + 1/n;
    System.out.println("O resultado de S é de : "+S);
    System.out.println("E o fatorial desse numero é de : "+n);
    
  }
}
